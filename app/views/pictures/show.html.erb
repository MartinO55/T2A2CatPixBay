<p id="notice"><%= notice %></p>

<p>
  <strong>Picture title:</strong>
  <%= @picture.picture_title %>
</p>

<p>
  <strong>Picture:</strong>
  <%= @picture.image %>
</p>

<p>
  <strong>Description:</strong>
  <%= @picture.description %>
</p>

<p>
  <strong>Non fungible token:</strong>
  <%= @picture.non_fungible_token %>
</p>

<p>
  <strong>Owner</strong>
